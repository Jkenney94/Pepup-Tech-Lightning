<aura:component controller="ContactListController" implements="flexipage:availableForAllPageTypes" access="global">
    <aura:attribute name="Contacts" type="Contact" />
    <aura:attribute name="Columns" type="List" />

    <aura:handler name="init" value="{!this}" action="{!c.myAction}" />

    <!--Version 1-->
    <lightning:card iconName="standard:contact" title="Contact List #1">
        <lightning:datatable data="{!v.Contacts}" columns="{!v.Columns}" keyField="Id" hideCheckboxColumn="true"/>
    </lightning:card>
</aura:component>